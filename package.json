{
  "name": "svelte",
  "version": "1.0.0",
  "devDependencies": {
    "less": "^3.11.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.27",
    "rollup": "^1.10.1",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-less": "^1.1.2",
    "rollup-plugin-node-resolve": "^4.2.3",
    "rollup-plugin-svelte": "^5.0.3",
    "rollup-plugin-terser": "^4.0.4",
    "sirv-cli": "^0.3.1",
    "svelte-preprocess": "^3.4.0",
    "svelte-preprocess-markdown": "^2.7.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "svelte": "^3.18.1",
    "svelte-routing": "^1.4.2"
  },
  "scripts": {
    "build": "rollup -c ./config/rollup.config.client.js",
    "build:ssr": "rollup -c ./config/rollup.config.ssr.js",
    "autobuild": "rollup -c ./config/rollup.config.client.js -w",
    "dev": "run-p start:dev autobuild",
    "start": "sirv public -s",
    "start:dev": "sirv public --dev -s"
  },
  "keywords": [
    "svelte",
    "starter"
  ],
  "description": "Svelte example starter project"
}
